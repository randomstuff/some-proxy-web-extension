{

  "manifest_version": 2,
  "name": "some-proxy-web-extension",
  "description": "Some extensions to configure proxy.",
  "version": "0.0.0",

  "icons": {
    "16": "icon.png"
},

  "browser_specific_settings": {
    "gecko": {
      "strict_min_version": "56.0a1"
    }
  },

  "background": {
    "scripts": [
      "background.js"
    ]
  },

  "permissions": ["proxy", "<all_urls>"]

}
